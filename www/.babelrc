{
    "presets": [
      "react", 
      ["env", {
        "targets": {
          "browsers": "last 2 versions"
        },
        "loose": true,
        "modules": false
      }]
    ],
    "ignore": ["node_modules"],
    "plugins": [
      "babel-plugin-transform-class-properties",
      "transform-object-rest-spread"
    ],
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-transform-es2015-modules-commonjs",
          "react-hot-loader/babel",
          "syntax-dynamic-import"
        ]
      }
    }
  }