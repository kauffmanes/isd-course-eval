{
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "globals": {
      "ROSLIB": true
    },
    "rules": {
      "no-new": 0,
      "no-plusplus": 0,
      "no-bitwise": 0,
      "import/prefer-default-export" : 0,
      "no-underscore-dangle": 0,
      "class-methods-use-this": 0,
      "dot-notation": 0,
      "no-nested-ternary": 0
    }
  }
  